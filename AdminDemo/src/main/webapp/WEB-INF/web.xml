<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
    <servlet>
    	<servlet-name>login</servlet-name>
    	<servlet-class>com.admin.LoginServlet</servlet-class>
    	
    </servlet> 
    <servlet-mapping>
    	<servlet-name>login</servlet-name>
    	<url-pattern>/log</url-pattern>
    
    </servlet-mapping>  
    
    <servlet>
    	<servlet-name>admininsert</servlet-name>
    	<servlet-class>com.admin.Admininsert</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>admininsert</servlet-name>
     	<url-pattern>/insert</url-pattern>
    
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>updateadmin</servlet-name>
     	<servlet-class>com.admin.UpdateAdminServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>updateadmin</servlet-name>
     	<url-pattern>/update</url-pattern>
     </servlet-mapping>
     
      <servlet>
     	<servlet-name>deleteadmin</servlet-name>
     	<servlet-class>com.admin.DeleteAdminServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>deleteadmin</servlet-name>
     	<url-pattern>/delete</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>schedule</servlet-name>
     	<servlet-class>com.admin.ScheduleServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>schedule</servlet-name>
     	<url-pattern>/scheduling</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>updateschedule</servlet-name>
     	<servlet-class>com.admin.UpdateScheduleServlet</servlet-class>
     </servlet>
      <servlet-mapping>
     	<servlet-name>updateschedule</servlet-name>
     	<url-pattern>/updatesche</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>deleteschedule</servlet-name>
     	<servlet-class>com.admin.DeleteScheduleServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>deleteschedule</servlet-name>
     	<url-pattern>/deletesche</url-pattern>
     </servlet-mapping>
         
</web-app>         